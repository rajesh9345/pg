-------------------menu----------------

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="menu.css">
    <script>
        var start=1;
        
        function set_time(){
           setInterval(img_show,3000);
        }
        function img_show(){
            var img;
            if(start==1){
                img="pratice1.jpeg";
            }
            else if(start==2){
                img="pratice2.jpg";
            }
           
            else if(start==3){
                img="redhonda3.jpg";
            }
           else{
               img="honda5.jpg";
               start=1;
           }
           document.getElementById("one").src=""+img;
           start++;
        }
    </script>
</head>
<body onload=set_time()>
    <div class="menu" >
        <ul>
            <li class="a"><a href="#">COMPANY</a>
            <div class="sub-menu">
                <ul>
                    
                    <li><a href="submenu.html">TATA</a></li>
                    <li><a href="firsthonda.html">H0NDA</a></li>
                    <li><a href="bmw.html">BMW</a></li> 
                </ul>
            </div>
            </li>
            <li class="b"><a href="#">price</a>
                <div class="p1">
            <ul>
                   <li><a href="below5.html">below 5,00,0000</a></li>
                   <li><a href="below10.html">below 10,00,0000</a></li>
                   <li><a href="below15.html">below 15,00,0000</a></li>
            </ul>
        </div>
            </li>
            <li ><a href="#">color</a></li>
            <li ><a href="#">engine</a></li>
            <li ><a href="#">model</a></li>
        </ul>
    </div>
    <div class="images">
        <img src="pratice1.jpeg" height="600" width="1600" id="one">
    </div>
</body>
</html>
-----------------menu css----------------
*{
    /* margin-left: 10px;
    margin-right: 10px; */
    box-sizing: border-box;
}
.menu{
    background-color: rgb(58, 114, 146);
    text-align: center;
}
.menu li{
    display: inline-flex;
    color:rgb(236, 233, 233);
}
.menu ul li{
    width: 120px;
    margin: 15px;
    padding: 15px;
}
.menu ul li a{
    text-decoration: none;
    list-style: none;
    color:#fff;
    
    margin: 0;
   
}
.a,.menu ul li:hover{
    background-color: #2ba;
    border-radius: 3px;
    color:crimson;
    
}
.sub-menu{
    display: none;
   
}
.p1{
    display: none;
}
.menu ul li:hover .sub-menu{
    display: block;
    position:absolute;
    color:black;
    background-color: rgb(173, 59, 69);
    margin-top:20px ;
    margin-left: -10px;
}
.menu ul li:hover .p1{
    display: block;
    position:absolute;
    color:black;
    background-color: rgb(167, 36, 55);
    margin-top:20px ;
    margin-left: -10px;
    
}
.menu ul li:hover .p1 li{
    display: block;
    margin:5px;
}
.menu ul li:hover .sub-menu li{
    display: block;
    margin:5px;
}
.menu ul li:hover .p1 ul li{
    width: 120px;
    padding: 15px;
    background:transparent;
    border-bottom: 1px dotted rgb(146, 44, 44);
    border-radius: 0;
    text-align: center;
}
.menu ul li:hover .sub-menu ul li{
    width: 80px;
    padding: 15px;
    background:transparent;
    border-bottom: 1px dotted #fff;
    border-radius: 0;
    text-align: center;
}
