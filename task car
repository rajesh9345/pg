        }
        h3{text-align: center;color:rebeccapurple}
        h2{text-align: center;color:blue}
    </style>
    <script>
        var start=1;
        
        function set_time(){
           setInterval(img_show,3000);
        }
        function img_show(){
            var img;
            if(start==1){
                img="bmwblue1.jpeg";
            }
            else if(start==2){
                img="bmwblue2.jpg";
            }
            
            else if(start==3){
                img="bmwblue3.jpg";
            }
           
           else{
            img="bmwblue2.jpg";
               start=1;
           }
           document.getElementById("one").src=""+img;
           start++;
        }
    </script>
</head>
