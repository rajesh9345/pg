-----------------bmw1 html------------------
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        .container{
            margin-left: 300px;
        }
        h3{text-align: center;color:rebeccapurple}
        h2{text-align: center;color:blue}
    </style>
    <script>
        var start=1;
        
        function set_time(){
           setInterval(img_show,3000);
        }
        function img_show(){
            var img;
            if(start==1){
                img="bmwblack1.jpg";
            }
            else if(start==2){
                img="bmwblack2.jpg";
            }
            // else if(start==3){
            //     img="pratice3.jpg";
            // }
            else if(start==3){
                img="bmwblack3.jpg";
            }
           
           else{
               img="bmwblack1.jpg";
               start=1;
           }
           document.getElementById("one").src=""+img;
           start++;
        }
    </script>
</head>
<body onload="set_time()">
