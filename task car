----------------second css------------
body{background-color: black;}
        .a1{
            background-color: black;
            color: turquoise;
            margin-left: 30px;
            margin-right: 30px;
            height: 40px;
            border: 2px rgb(192, 146, 60) dotted;
        }
        .a1 h3{text-align: center;}
        .first{
           background-color: white;
            float: left;
            width: 28%;
            margin-left: 50px;
            margin-top: 70px;
            border: 2px rgb(219, 217, 212) solid;
        }
        .second{
           background-color: white;
            float: left;
            width: 28%;
            margin-left: 50px;
            margin-top: 70px;
            border: 2px rgb(219, 217, 212) solid;
        }
        .third{
           background-color: white;
            float: left;
            width: 30%;
            margin-left: 50px;
            margin-top: 70px;
            border: 2px rgb(219, 217, 212) solid;
        }
        .a2{background-color: rgb(246, 246, 12);
            rotate: kanna;
         color:black;
         margin-left: 250px;
         margin-right: 20px;
         height: 50px;
        }
        .a2 h2{text-align: center;font-size: x-large;margin-top: 20px;}
        
        -------------------submenu2 html------------------
        <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        .container{
            margin-left: 300px;
        }
        h3{text-align: center;color:rebeccapurple}
        h2{text-align: center;color:blue}
    </style>
    <script>
        var start=1;
        
        function set_time(){
           setInterval(img_show,3000);
        }
        function img_show(){
            var img;
            if(start==1){
                img="blue1.webp";
            }
            else if(start==2){
                img="blue4.jpg";
            }
            // else if(start==3){
            //     img="pratice3.jpg";
            // }
            else if(start==3){
                img="blue3.webp";
            }
           else{
               start=1;
           }
           document.getElementById("one").src=""+img;
           start++;
        }
    </script>
</head>
<body onload="set_time()">
    <div class="container">
    <img src="blue1.webp"height="500" width="900" id="one">
</div>
<div>
    <h3>Tata model:XX123</h3>
    <h3>prize::14,40,000</h3>
    <h2>Tata best model in india</h2>
</div>
</body>
</html>
